CREATE TABLE diarios
(
  id INT
, nome VARCHAR(200)
, disciplina_id INT
, periodo_letivo INT
, recuperacao_final CHAR(1)
, aulas_dadas INT
, data_liberacao BOOLEAN
, data_limite BOOLEAN
, data_entrega DATETIME
, pessoa_entrega_id INT
, data_homologacao BOOLEAN
, pessoa_homologacao_id BOOLEAN
, relatorio BOOLEAN
, entidade_id INT
, created_at DATETIME
, updated_at DATETIME
, aguardando_devolucao CHAR(1)
, recuperacao_semestral CHAR(1)
, resultado_multiplicado INT
, periodo_regular CHAR(1)
, prova_final CHAR(1)
, prova_especial CHAR(1)
, conselho_classe CHAR(1)
, data_atualizacao BOOLEAN
, codigo_migracao TEXT
, codigo_migracao_2 TEXT
, INDEX `id`(`id`) USING BTREE 
, INDEX `disciplina_id`(`disciplina_id`) USING BTREE
)
;</sql>
