LEFT JOIN Sophia.RELIGIOES RE ON A2.RELIGIAO = RE.CODIGO
LEFT JOIN Sophia.RACA R ON A.RACA = R.CODIGO
LEFT JOIN Sophia.ESTCIVIL ETC ON A2.EST_CIVIL = ETC.CODIGO
LEFT JOIN sophia.OCUPACOES ocp ON (ocp.CODIGO = fa.PAI_OCUPACAO)
LEFT JOIN sophia.OCUPACOES ocm ON (ocm.CODIGO = fa.MAE_OCUPACAO)
WHERE A.CPF IS NOT NULL AND LEN(A.cpf) > 1 AND a.NOME IS NOT NULL;
