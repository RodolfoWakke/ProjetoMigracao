    <sql>-- RESPONSÁVEL --
--PAI 
SELECT 
	DISTINCT TRIM(dbo.REMOVEcaractere(fa.pai_CPF)) AS 'cpf', 
	TRIM(UPPER(fa.pai_NOME)) AS 'nomeresponsavel', 
	UPPER(o.DESCRICAO) AS 'profissao', 
	TRIM(dbo.REMOVEcaractere(fa.pai_ER_CONTATO1)) AS 'celular', 
	TRIM(dbo.REMOVEcaractere(fa.PAI_ER_CONTATO2)) AS 'telresid',
	TRIM(fa.pai_EMAIL) AS 'email',
	fa.pai_RG AS 'rg', 
	UPPER(ISNULL(fa.pai_ER_ENDERECO,fa.pai_EC_ENDERECO)) AS 'endereco', 
	UPPER(ba.DESCRICAO) AS 'bairro',
	cc.cod_cidade AS 'cidade',
	LEFT(TRIM(dbo.REMOVEcaractere(fa.pai_ER_CEP)),8) AS 'cep',
	'M' AS 'sexo', 
	UPPER(et.DESCRICAO) AS 'estadocivil',
	CAST(fa.pai_DATA_NASC AS DATE) AS 'dtnascimento',
	fa.pai_ORGAO_EXP AS 'rgorgaoemissor',
	'MIG AUTO' AS 'cadastradopor',
	CAST(GETDATE() AS DATE) AS 'dtcadastro'
FROM sophia.fisica f
LEFT JOIN sophia.DADOSPF d ON (d.FISICA = f.CODIGO)
LEFT JOIN sophia.FAMILIAS fa ON (d.FAMILIA = fa.CODIGO)
LEFT JOIN sophia.OCUPACOES o ON (fa.pai_OCUPACAO = o.CODIGO)
LEFT JOIN sophia.CIDADES c ON (fa.pai_ER_CIDADE = c.CODIGO)
LEFT JOIN sophia.ESTCIVIL et ON (et.CODIGO = fa.pai_EST_CIVIL)
LEFT JOIN sophia.BAIRROS ba ON (ISNULL(fa.PAI_EC_BAIRRO,fa.PAI_ER_BAIRRO) = ba.CODIGO)
LEFT JOIN cidade_escolaweb cc ON (c.DESCRICAO = cc.nom_cidade)
WHERE fa.pai_CPF IS NOT NULL AND LEN(fa.pai_CPF) > 1 AND fa.PAI_NOME IS NOT NULL

UNION ALL

-- MAE
SELECT 
	DISTINCT RTRIM(dbo.REMOVEcaractere(fa.MAE_CPF)) AS 'cpf', 
	TRIM(UPPER(fa.MAE_NOME)) AS 'nomeresponsavel', 
	UPPER(o.DESCRICAO) AS 'profissao', 
	RTRIM(dbo.REMOVEcaractere(fa.MAE_ER_CONTATO2)) AS 'telresid', 
	RTRIM(dbo.REMOVEcaractere(fa.MAE_ER_CONTATO1)) AS 'celular',
	TRIM(fa.MAE_EMAIL) AS 'email',
	fa.MAE_RG AS 'rg', 
	UPPER(ISNULL(fa.MAE_ER_ENDERECO,fa.MAE_EC_ENDERECO)) AS 'endereco', 
	UPPER(ba.DESCRICAO) AS 'bairro',
	cc.cod_cidade AS 'cidade',
	LEFT(RTRIM(dbo.REMOVEcaractere(fa.MAE_ER_CEP)),8) AS 'cep',
	'F' AS 'sexo', 
	UPPER(et.DESCRICAO) AS 'estadocivil',
	CAST(fa.MAE_DATA_NASC AS DATE) AS 'dtnascimento',
	fa.MAE_ORGAO_EXP AS 'rgorgaoemissor',
	'MIG AUTO' AS 'cadastradopor', 
	CAST(GETDATE() AS DATE) AS 'dtcadastro'
FROM sophia.fisica f
LEFT JOIN sophia.DADOSPF d ON (d.FISICA = f.CODIGO)
LEFT JOIN sophia.FAMILIAS fa ON (d.FAMILIA = fa.CODIGO)
LEFT JOIN sophia.OCUPACOES o ON (fa.MAE_OCUPACAO = o.CODIGO)
LEFT JOIN sophia.CIDADES c ON (fa.MAE_ER_CIDADE = c.CODIGO)
LEFT JOIN sophia.ESTCIVIL et ON (et.CODIGO = fa.MAE_EST_CIVIL)
LEFT JOIN sophia.BAIRROS ba ON (ISNULL(fa.MAE_EC_BAIRRO,fa.MAE_ER_BAIRRO) = ba.CODIGO)
LEFT JOIN cidade_escolaweb cc ON (c.DESCRICAO = cc.nom_cidade)
WHERE fa.MAE_CPF IS NOT NULL AND LEN(fa.MAE_CPF) > 1 AND fa.mae_nome IS NOT NULL

UNION ALL

-- OUTROS
SELECT 
DISTINCT 
	TRIM(dbo.REMOVEcaractere(CPF)) AS 'cpf',
	TRIM(REPLACE(RTRIM(UPPER(NOME)), CHAR(9), '')) AS 'nomeresponsavel',
	'' AS 'profissao', 
	dbo.REMOVEcaractere(CONTATO1) AS 'telresid', 
	dbo.REMOVEcaractere(CONTATO2) AS 'celular', 
	TRIM(EMAIL) AS 'email',
	REPLACE(RG, CHAR(9), '') AS 'RG', 
	UPPER(IIF(LOGRADOURO IS NULL, NULL,(ISNULL(LOGRADOURO,'')+', '+ ISNULL(NUMERO,'')+' '+ ISNULL(COMPLEMENTO,'')))) AS 'endereco', 
	UPPER(B.DESCRICAO) AS 'bairro', 
	cc.cod_cidade AS 'cidade',
	LEFT(RTRIM(dbo.REMOVEcaractere(CEP)),8) AS 'cep',
	SEXO AS 'sexo',
	'' AS 'estadocivil',
	CAST(DATANASC AS DATE) AS 'dtnascimento', 
	ORGAOEXP AS 'rgorgaoemissor',
	'MIG AUTO' AS 'cadastradopor', 
	CAST(GETDATE() AS DATE) AS 'dtcadastro'
FROM sophia.FISICA A
LEFT JOIN Sophia.DADOSPF A2 ON A.CODIGO = A2.FISICA
LEFT JOIN sophia.FAMILIAS fa ON a2.FAMILIA = fa.CODIGO
LEFT JOIN Sophia.BAIRROS B ON A.BAIRRO = B.CODIGO
LEFT JOIN Sophia.BAIRROS BM ON FA.MAE_ER_BAIRRO = BM.CODIGO
LEFT JOIN Sophia.BAIRROS BP ON FA.PAI_ER_BAIRRO = BP.CODIGO
LEFT JOIN Sophia.CIDADES C ON A.CIDADE = C.CODIGO
LEFT JOIN cidade_escolaweb cc ON (c.DESCRICAO = cc.nom_cidade)
LEFT JOIN Sophia.CIDADES CM ON fa.MAE_ER_CIDADE = CM.CODIGO
LEFT JOIN Sophia.CIDADES CP ON fa.PAI_ER_CIDADE = CP.CODIGO
LEFT JOIN Sophia.CIDADES C2 ON A2.LOCAL_NASC = C2.CODIGO
