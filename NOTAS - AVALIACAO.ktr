-- NOTAS 1º BIMESTRE 

INSERT INTO notas (codserie, codturma, codmateria, codetapa, matricula, ano, n1, codprofturma, totalnotas)
(select
a.CODSERIE,
a.CODTURMA, 
a.CODMATERIA, 
1 as 'codetapa',
a.MATRICULA,
a.ANO,
a.MEDIA1 as 'n1',
a.codprofturma,
a.MEDIA1 as 'totalnotas'
from acumulado a
WHERE a.ANO = ${v_notas_anoletivo}
) ;

