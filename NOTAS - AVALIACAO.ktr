    <sql>DELETE FROM notas;
DELETE FROM avaliacao;
DELETE e FROM esquemaavaliacao e WHERE e.codesquemaav = 1;
DELETE e FROM esquema e WHERE e.codesquema = 3;


ALTER TABLE notas AUTO_INCREMENT = 1;
ALTER TABLE avaliacao AUTO_INCREMENT = 1;


