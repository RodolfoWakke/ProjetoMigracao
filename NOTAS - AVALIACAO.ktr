-- AVALIAÇAO 1º BIMESTRE

INSERT INTO avaliacao (codserie, codturma, codmateria, codetapa, ano, dtregistro, dscavaliacao, valor, coluna, incluidosistema, dscdetalhada, codprofturma, codesquemaav, cadastradopor, codesquema)
(select
p.CODSERIE,
p.CODTURMA,
p.CODMATERIA,
1 as 'codetapa',
p.ANO,
now() as 'dtregistro',
'MIGPR' as dscavaliacao,
${v_notas_avaliacao} as 'valor',
'N1' as 'coluna',
'S' as 'incluidosistema',
'MÉDIA DA ETAPA (MIG AUTO)' as 'dscdetalhada',
p.CODPROFTURMA,
1 AS 'codesquemaav',
'MIG AUTO' as 'cadastradopor',
3 AS 'codesquema'
from acumulado a
left join professorturma p on (a.codprofturma = p.CODPROFTURMA)
WHERE p.ANO = ${v_notas_anoletivo}
group by p.CODPROFTURMA) ;

