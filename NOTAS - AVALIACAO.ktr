DELETE a FROM avaliacao a WHERE a.codetapa IN (
SELECT n.codetapa FROM notas n
JOIN (
	SELECT n.codetapa, AVG(n.n1) AS avg_n1
	FROM notas n
	GROUP BY n.codetapa
) AS b ON n.codetapa = b.codetapa
WHERE b.avg_n1 = 0 AND a.dscavaliacao = 'MIGPR'
GROUP BY n.codetapa)
;


