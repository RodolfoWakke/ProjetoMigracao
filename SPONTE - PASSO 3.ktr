    <sql>-- AJUSTAR TOTPARCELA

UPDATE mig_contasreceber c
LEFT JOIN (SELECT codmatricula, MAX(parcela) AS totparcela
FROM mig_contasreceber
GROUP BY codmatricula) d
ON c.codmatricula = d.codmatricula
SET c.totparcela = d.totparcela;


UPDATE mig_contasreceber c
SET c.totparcela = c.parcela
WHERE c.codmatricula IS NULL;


UPDATE mig_contasreceber c
JOIN mig_centroreceita cr ON (c.AUX_codreceita = cr.codreceita_aux)
SET c.codreceita = cr.codreceita, c.receitaoriginal = cr.codreceita;</sql>
