    <sql>update configturma c
set c.idconfigetapa = NULL ;

DELETE FROM configetapaitem;
DELETE FROM configetapa;
DELETE FROM etapaturma;

ALTER TABLE configetapaitem AUTO_INCREMENT = 1;
ALTER TABLE configetapa AUTO_INCREMENT = 1;
ALTER TABLE etapaturma AUTO_INCREMENT = 1;

-- 1º SEMESTRE

INSERT INTO etapaturma(codconfturma , dscetapa , ordem , dtinicio , dtfinal , valoretapa , mediaetapa , anoletivo , liberado , peso)
(SELECT c.codconfturma, 'ETAPA 1' AS dscetapa, 1 as ordem, CONCAT(c.anoletivo,'-02-01') as dtinicio, CONCAT(c.anoletivo,'-04-17') as dtfinal, 
10 as valoretapa, 6 as mediaetapa, c.anoletivo, 'N' as liberado, 1 as peso FROM configturma c, seriecurso sc 
where sc.codseriecurso = c.codseriecurso );

-- 2º SEMESTRE 

INSERT INTO etapaturma(codconfturma , dscetapa , ordem , dtinicio , dtfinal , valoretapa , mediaetapa , anoletivo , liberado , peso) 
(SELECT c.codconfturma, 'ETAPA 2' AS dscetapa, 2 as ordem, CONCAT(c.anoletivo,'-04-18') as dtinicio, CONCAT(c.anoletivo,'-07-30') as dtfinal, 
10 as valoretapa, 6 as mediaetapa, c.anoletivo, 'N' as liberado, 1 as peso FROM configturma c, seriecurso sc 
where sc.codseriecurso = c.codseriecurso );


-- INSERINDO CONFIGETAPA

INSERT INTO configetapa (dscconfetapa,cadastradopor,dtcadastro)
(select 
CONCAT('ETAPAS ',e.anoletivo) as dscconfetapa, 
'MIG AUTO' as cadastradopor ,
now() as 'dtcadastro'
from etapaturma e 
join configturma c on (c.codconfturma = e.codconfturma)
join seriecurso s on (s.codseriecurso = c.codseriecurso)
where e.idconfigetapa is null
group by e.anoletivo);


-- SETANDO O IDCONFIGETAPA NO ETAPATURMA (SEMESTRE) -- REVER JOIN
UPDATE etapaturma e
JOIN configetapa c ON (c.dscconfetapa = CONCAT(LEFT(e.dscetapa,5),'S', ' ', e.anoletivo))
SET e.idconfigetapa = c.codconfetapa
where e.idconfigetapa is NULL ;



-- INSERINDO O CONFIGETAPAITEM

INSERT INTO configetapaitem (codconfetapa, dscetapa, ordem, dtinicio, dtfinal, valoretapa, mediaetapa, peso, alteradopor)
(select
idconfigetapa, CONCAT(dscetapa,' ', e.anoletivo), ordem, dtinicio, dtfinal, valoretapa, mediaetapa, peso, 'MIG AUTO'
from etapaturma e
group by e.idconfigetapa, e.ordem) ;



-- SETANDO IDCONFIGETAPA NO CONFIGTURMA

UPDATE configturma t
INNER JOIN configetapa e ON (t.anoletivo = RIGHT(e.dscconfetapa, 4))
SET t.idconfigetapa = e.codconfetapa</sql>
